<%- model_class = Inventario -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.titleize %></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name(:pieza) %>:</strong></dt>
  <dd><%= @inventario.pieza %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Disponible) %>:</strong></dt>
  <dd><%= @inventario.Disponible %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Usada) %>:</strong></dt>
  <dd><%= @inventario.Usada %></dd>
  <dt><strong><%= model_class.human_attribute_name(:Entrada) %>:</strong></dt>
  <dd><%= @inventario.Entrada %></dd>
</dl>

<%= link_to t('.back', :default => t("helpers.links.back")),
              inventarios_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_inventario_path(@inventario), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              inventario_path(@inventario),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
